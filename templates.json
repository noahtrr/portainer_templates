{
  "version": "3",
  "templates": [
    {
      "id": 1,
      "type": 1,
      "title": "Traefik",
      "description": "Traefik is a modern HTTP reverse proxy and load balancer that makes deploying services easy.",
      "image": "traefik:latest",
      "name": "Traefik",
      "logo": "https://cdn.jsdelivr.net/gh/selfhst/icons/svg/traefik.svg",
      "env": [
        {
          "name": "TRAEFIK_API_DASHBOARD",
          "default": "true",
          "preset": true
        }
      ],
      "network": "proxy",
      "volumes": [
        {
          "container": "/letsencrypt"
        }
      ],
      "ports": ["8080:8080/tcp", "80:80/tcp", "443:443/tcp"],
      "labels": [
        { "name": "traefik.enable", "value": "true" }
      ],
      "restart_policy": "unless-stopped",
      "hostname": "traefik",
      "platform": "linux",
      "categories": ["networking", "proxy"]
    }
  ]
}