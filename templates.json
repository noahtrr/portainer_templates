{
  "version": "3",
  "templates": [
    {
      "id": 1,
      "type": 1,
      "title": "Traefik",
      "description": "Traefik Reverse Proxy and Load Balancer",
      "categories": ["networking", "proxy"],
      "platform": "linux",
      "logo": "https://cdn.jsdelivr.net/gh/selfhst/icons/webp/traefik-light.svg",
      "image": "traefik:v3.5.3",
      "ports": ["80:80/tcp", "443:443/tcp", "8080:8080/tcp"],
      "volumes": [
        {
          "container": "/certs"
        },
        {
            "container": "/var/run/docker.sock",
            "bind": "/var/run/docker.sock"
        }
      ],
      "network": "proxy",
      "restart_policy": "unless-stopped",
      "hostname": "traefik"
    },
    {
      "id": 2,
      "type": 1,
      "title": "Pocket-ID",
      "description": "OIDC Identity Provider for your applications",
      "categories": ["security", "IDP"],
      "platform": "linux",
      "logo": "https://cdn.jsdelivr.net/gh/selfhst/icons/webp/pocket-id-light.svg",
      "image": "ghcr.io/pocket-id/pocket-id:v1",
      "ports": ["1411:1411/tcp"],
      "volumes": [
        {
          "container": "/app/data"
        }
      ],
      "network": "proxy",
      "restart_policy": "unless-stopped",
      "hostname": "pocket-id",
        "env": [
    {
      "name": "APP_URL",
      "label": "Pocket ID Application URL"
    },
    {
      "name": "TRUST_PROXY",
      "label": "Trust Proxy",
      "select": [
        {
          "text": "Yes",
          "value": "true"
        },
        {
          "text": "No",
          "value": "false",
          "default": true
        }
    ]
    }
  ]
    }
    ]
}